<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="snt_debt_type_action" model="ir.actions.act_window">
        <field name="name">Debt Type</field>
        <field name="res_model">snt.debt.type</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="snt_debt-type_form" model="ir.ui.view">
        <field name="name">snt.debt.type.form</field>
        <field name="model">snt.debt.type</field>
        <field name="arch" type="xml">
            <form string="Debt type">
                <sheet>
                    <field name="name"/>
                    <field name="description"/>
                </sheet>
            </form>
        </field>
    </record>
    <record id="snt_debt-type_tree" model="ir.ui.view">
        <field name="name">snt.debt.type.tree</field>
        <field name="model">snt.debt.type</field>
        <field name="arch" type="xml">
            <tree string="Debt type">
                <field name="name"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    
</odoo>